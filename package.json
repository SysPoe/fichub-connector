  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SysPoe/fichub-connector.git"
  },
  "author": "SysPoe",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/SysPoe/fichub-connector/issues"
  },
